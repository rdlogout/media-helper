{
	"name": "media-helper",
	"version": "1.0.0",
	"type": "module",
	"packageManager": "pnpm@9.1.0",
	"scripts": {
		"dev": "bun run --hot src/index.ts",
		"deploy": "export TAG=0.5 && docker build --platform linux/amd64 -t base15log/media:$TAG . && docker push base15log/media:$TAG"
	},
	"dependencies": {
		"@cf-wasm/photon": "^0.2.1",
		"@types/fluent-ffmpeg": "^2.1.28",
		"fluent-ffmpeg": "^2.1.3",
		"hono": "^4.10.4",
		"sharp": "^0.34.5"
	},
	"pnpm": {
		"supportedArchitectures": {
			"os": [
				"linux",
				"darwin",
				"win32"
			],
			"cpu": [
				"x64",
				"arm64"
			],
			"libc": [
				"glibc",
				"musl"
			]
		}
	},
	"devDependencies": {
		"@types/bun": "latest"
	}
}
